FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     icoFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         {{endTime}};

deltaT          {{timeStep}};

writeControl    timeStep;

writeInterval   {{writeInterval}};

purgeWrite      0;

writeFormat     {{writeFormat}};

writePrecision  {{writePrecision}};

writeCompression off;

timeFormat      general;

timePrecision   {{timePrecision}};

runTimeModifiable false;


